  "homepage": "https://github.com/BeepBoopHQ/starter-slapp-app#readme",
  "dependencies": {
    "express": "^4.14.0",
    "firebase": "^3.9.0",
    "nodemon": "^1.11.0",
    "slapp": "^2.1.0",
    "slapp-context-beepboop": "^1.3.0",
    "slapp-convo-beepboop": "^1.0.1",
    "standard": "^8.4.0"
  },
  "devDependencies": {
    "husky": "^0.13.3",
    "lint-staged": "^3.4.1",
    "prettier": "^1.2.2"
